{
    "cli": {
      "version": ">= 5.0.0"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "android": {
          "gradleCommand": ":app:assembleDebug",
          "buildType": "apk"
        }
      },
      "preview": {
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        },
        "env": {
          "EXPO_PUBLIC_STAGE": "dev"
        }
      },
      "production": {
        "android": {
          "buildType": "apk"
        },
        "env": {
          "EXPO_PUBLIC_STAGE": "prod"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }