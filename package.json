{
  "name": "medisupply-movil-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:build": "expo export --platform web",
    "web:serve": "yarn dlx serve dist",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "diagnose": "node ./scripts/diagnose-connectivity.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:host": "docker-compose -f docker-compose.host.yml up",
    "docker:dev:host:build": "docker-compose -f docker-compose.host.yml up --build",
    "docker:dev:api": "docker-compose -f docker-compose.dev.yml --profile api up",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:mobile": "docker-compose --profile mobile up medisupply-mobile",
    "docker:prod": "docker-compose --profile production up medisupply-prod",
    "docker:build": "docker build -f Dockerfile.dev -t medisupply-dev ."
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.90.3",
    "axios": "^1.11.0",
    "expo": "~54.0.1",
    "expo-constants": "~18.0.8",
    "expo-font": "~14.0.7",
    "expo-haptics": "~15.0.6",
    "expo-image": "~3.0.7",
    "expo-linking": "~8.0.7",
    "expo-localization": "^17.0.7",
    "expo-router": "~6.0.0",
    "expo-secure-store": "^15.0.6",
    "expo-splash-screen": "~31.0.8",
    "expo-status-bar": "~3.0.7",
    "expo-symbols": "~1.0.6",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.6",
    "i18next": "^25.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.3.3",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "babel-jest": "^29.2.1",
    "cypress": "^15.4.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-expo": "^54.0.12",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
