{
    "request": {
        "method": "POST",
        "url": "/api/auth/login"
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": "application/json"
        },
        "jsonBody": {
            "success": true,
            "data": {
                "token": "mock-jwt-token-123456789",
                "user": {
                    "id": 1,
                    "email": "{{request.body.email}}",
                    "name": "Usuario Demo",
                    "role": "admin"
                }
            }
        }
    }
}