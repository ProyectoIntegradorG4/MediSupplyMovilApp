# Environment variables for development
# Copy this file to .env.local and modify values as needed

# === APPLICATION ENVIRONMENT ===
NODE_ENV=development
EXPO_PUBLIC_STAGE=dev

# === API CONFIGURATION ===
# Development API URLs for different platforms
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_API_URL_ANDROID=http://host.docker.internal:3000/api
EXPO_PUBLIC_API_URL_IOS=http://host.docker.internal:3000/api

# Production API URL (only use in production)
# EXPO_PUBLIC_API_URL=https://api.medisupply.com

# === EXPO CONFIGURATION ===
EXPO_DEVTOOLS_LISTEN_ADDRESS=0.0.0.0
EXPO_DEBUG=true

# === AUTHENTICATION ===
# Add any authentication keys or secrets here
# JWT_SECRET=your-jwt-secret-here
# GOOGLE_CLIENT_ID=your-google-client-id
# FACEBOOK_APP_ID=your-facebook-app-id

# === THIRD PARTY SERVICES ===
# Add configuration for external services
# SENTRY_DSN=your-sentry-dsn
# ANALYTICS_KEY=your-analytics-key

# === DATABASE (if using backend services) ===
# DATABASE_URL=postgresql://user:password@localhost:5432/medisupply
# REDIS_URL=redis://localhost:6379

# === FEATURE FLAGS ===
# ENABLE_PUSH_NOTIFICATIONS=true
# ENABLE_BIOMETRIC_AUTH=true
# ENABLE_OFFLINE_MODE=true



EXPO_PUBLIC_STAGE=dev

# API Gateway URL (puerto 80, sin especificar puerto)
EXPO_PUBLIC_GATEWAY_URL_ANDROID=http://10.0.2.2
EXPO_PUBLIC_GATEWAY_URL_IOS=http://192.168.1.110
EXPO_PUBLIC_GATEWAY_URL=http://192.168.1.110

# URLs legacy (mantener por compatibilidad, pero ya no se usan directamente)
# EXPO_PUBLIC_API_URL_ANDROID=http://10.0.2.2:8001
# EXPO_PUBLIC_API_URL_IOS=http://192.168.101.90:8001
# EXPO_PUBLIC_API_URL=http://192.168.101.90:8001